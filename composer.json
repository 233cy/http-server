{
    "name": "hyperf/http-server",
    "description": "A HTTP Server for Hyperf.",
    "license": "Apache-2.0",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "http",
        "http-server"
    ],
    "support": {
    },
    "require": {
        "php": ">=7.1",
        "nikic/fast-route": "^1.3",
        "hyperf/framework": "dev-master",
        "hyperf/utils": "dev-master",
        "swoft/http-message": "^1.0"
    },
    "require-dev": {
        "malukenho/docheader": "^0.1.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0.0",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "suggest": {
    },
    "autoload": {
        "files": [
        ],
        "psr-4": {
            "Hyperf\\HttpServer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
        },
        "hyperf": {
            "config": "Hyperf\\HttpServer\\ConfigProvider"
        }
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
